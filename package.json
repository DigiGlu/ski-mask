    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/ajv": "^1.0.0",
    "ajv": "^5.5.1",
    "commander": "^2.12.2",
    "fp-ts": "^0.6.4",
    "jsome": "^2.3.26",
    "ramda": "^0.25.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  },
  "scripts": {
    "test": "nyc mocha -r ts-node/register -r source-map-support/register 'src/**/*.test.ts' --timeout=10000",
    "compile": "tsc && babel-node ./scripts/syncJsonFiles.js && chmod +x ./dist/commandLine.js",
    "release": "yarn compile && yarn test && yarn version && yarn publish"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "syncJsonFiles.js",
      "**/*.d.ts",
      "**/__test__/*",
      "dist/**/*",
      "coverage/**/*"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
