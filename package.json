    "@types/commander": "^2.11.0",
    "@types/jsen": "^0.0.19",
    "@types/mocha": "^2.2.44",
    "@types/ramda": "^0.25.6",
    "@types/request-promise-native": "^1.0.10",
    "@types/sinon": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
